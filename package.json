                "react-dom": "18"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.nixdev.co/foxes-knowledge/foxes-knowledge.git"
    },
    "bugs": {
        "url": "https://gitlab.nixdev.co/foxes-knowledge/foxes-knowledge/issues"
    },
    "homepage": "https://gitlab.nixdev.co/foxes-knowledge/foxes-knowledge#readme",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"

        "node-emoji": "^1.11.0",
        "nprogress": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.2",
        "reading-time": "^1.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/types": "^17.0.0",
        "@types/node": "^18.0.0",
        "@types/node-emoji": "^1.8.1",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "commitlint-config-gitmoji": "^2.2.5",
        "cypress": "^10.3.0",
        "eslint": "^8.18.0",
        "eslint-config-next": "^12.2.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "typescript": "^4.7.4"
    },
    "engines": {

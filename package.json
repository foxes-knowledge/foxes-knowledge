        "cypress": "cypress open",
        "cypress:headless": "cypress run"
    },
    "dependencies": {
        "dayjs": "^1.11.4",
        "iron-session": "^6.1.3",
        "markdown-to-jsx": "^7.1.7",
        "next": "12.2.3",
        "nprogress": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-select": "^5.4.0",
        "react-textarea-autosize": "^8.3.4",
        "zustand": "4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@types/node": "^18.6.1",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "commitlint-config-gitmoji": "^2.2.5",
        "cypress": "^10.3.1",
        "eslint": "^8.20.0",
        "eslint-config-next": "^12.2.3",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "sass": "^1.54.0",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=18",
        "pnpm": ">=7"
    },
    "pnpm": {
        "peerDependencyRules": {
